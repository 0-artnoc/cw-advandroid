<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.commonsware.android.camera" android:versionCode="1" android:versionName="1.0">
 <uses-sdk android:minSdkVersion="5" android:targetSdkVersion="6"/> 
	<supports-screens android:largeScreens="false" android:normalScreens="true" android:smallScreens="false"/>
	<uses-feature android:name="android.hardware.camera"/>
	<uses-feature android:name="android.hardware.camera.front" android:required="false"/>
	<uses-permission android:name="android.permission.CAMERA"/>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
		<application android:label="@string/app_name" android:icon="@drawable/cw">
				<activity android:name=".PictureDemo" android:label="@string/app_name" android:configChanges="keyboardHidden|orientation" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
						<intent-filter>
								<action android:name="android.intent.action.MAIN"/>
								<category android:name="android.intent.category.LAUNCHER"/>
						</intent-filter>
				</activity>
		</application>
</manifest>
